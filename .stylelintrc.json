{
  "extends": ["stylelint-config-standard"],
  "rules": {
    "selector-pseudo-element-colon-notation": "double",
    "selector-type-no-unknown": [
      true,
      {
        "ignoreTypes": ["page"]
      }
    ],
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": ["import"]
        }
      ]
  }
}
